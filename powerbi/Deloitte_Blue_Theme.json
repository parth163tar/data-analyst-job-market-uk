{
  "name": "Deloitte Blue Professional v2",
  "dataColors": [
    "#0050A0",
    "#00A0D6",
    "#1F2E3D",
    "#7BAFD4",
    "#50C0E9",
    "#8796A5",
    "#C0CCD9",
    "#F2B134"
  ],
  "background": "#FFFFFF",
  "foreground": "#1F2E3D",
  "tableAccent": "#0050A0",
  "visualStyles": {
    "*": {
      "*": {
        "border": [
          {
            "show": true,
            "color": { "solid": { "color": "#D3D3D3" } },
            "radius": 4
          }
        ],
        "title": [
          {
            "show": true,
            "fontSize": 12,
            "fontFamily": "Segoe UI",
            "fontColor": { "solid": { "color": "#1F2E3D" } }
          }
        ],
        "background": [
          {
            "show": true,
            "color": { "solid": { "color": "#FFFFFF" } },
            "transparency": 0
          }
        ]
      }
    },
    "card": {
      "*": {
        "title": [
          {
            "show": false
          }
        ],
        "categoryLabels": [
          {
            "show": true,
            "color": { "solid": { "color": "#6C7884" } },
            "fontSize": 11,
            "fontFamily": "Segoe UI Semibold"
          }
        ],
        "labels": [
          {
            "color": { "solid": { "color": "#1F2E3D" } },
            "fontSize": 26,
            "fontFamily": "Segoe UI Semibold"
          }
        ]
      }
    },
    "slicer": {
      "*": {
        "title": [
          {
            "show": true,
            "fontSize": 12,
            "fontFamily": "Segoe UI Semibold",
            "fontColor": { "solid": { "color": "#1F2E3D" } }
          }
        ],
        "items": [
          {
            "fontColor": { "solid": { "color": "#4C5C68" } },
            "fontSize": 11,
            "fontFamily": "Segoe UI"
          }
        ],
        "background": [
          {
            "show": true,
            "color": { "solid": { "color": "#FFFFFF" } },
            "transparency": 0
          }
        ]
      }
    },
    "clusteredBarChart": {
      "*": {
        "dataLabels": [
          {
            "show": true,
            "color": { "solid": { "color": "#1F2E3D" } },
            "fontSize": 10,
            "fontFamily": "Segoe UI"
          }
        ],
        "categoryAxis": [
          {
            "show": true,
            "fontSize": 11,
            "fontFamily": "Segoe UI",
            "labelColor": { "solid": { "color": "#4C5C68" } }
          }
        ],
        "valueAxis": [
          {
            "show": true,
            "fontSize": 11,
            "fontFamily": "Segoe UI",
            "labelColor": { "solid": { "color": "#4C5C68" } },
            "gridlineColor": { "solid": { "color": "#E3E6EA" } }
          }
        ]
      }
    },
    "clusteredColumnChart": {
      "*": {
        "dataLabels": [
          {
            "show": true,
            "color": { "solid": { "color": "#1F2E3D" } },
            "fontSize": 10,
            "fontFamily": "Segoe UI"
          }
        ],
        "categoryAxis": [
          {
            "show": true,
            "fontSize": 11,
            "fontFamily": "Segoe UI",
            "labelColor": { "solid": { "color": "#4C5C68" } }
          }
        ],
        "valueAxis": [
          {
            "show": true,
            "fontSize": 11,
            "fontFamily": "Segoe UI",
            "labelColor": { "solid": { "color": "#4C5C68" } },
            "gridlineColor": { "solid": { "color": "#E3E6EA" } }
          }
        ]
      }
    },
    "lineChart": {
      "*": {
        "dataLabels": [
          {
            "show": false,
            "color": { "solid": { "color": "#1F2E3D" } },
            "fontSize": 10,
            "fontFamily": "Segoe UI"
          }
        ],
        "categoryAxis": [
          {
            "show": true,
            "fontSize": 11,
            "fontFamily": "Segoe UI",
            "labelColor": { "solid": { "color": "#4C5C68" } }
          }
        ],
        "valueAxis": [
          {
            "show": true,
            "fontSize": 11,
            "fontFamily": "Segoe UI",
            "labelColor": { "solid": { "color": "#4C5C68" } },
            "gridlineColor": { "solid": { "color": "#E3E6EA" } }
          }
        ]
      }
    }
  }
}
